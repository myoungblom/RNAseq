universe = vanilla
executable = multiQC.sh
arguments = "$(RUN)"

output = multiqc_$(RUN).out
error = multiqc_$(RUN).err
log = multiqc_$(RUN).log

should_transfer_files = YES
when_to_transfer_output = ON_EXIT
transfer_input_files = $(RUN).sort.bam

request_cpus = 4
request_memory = 2GB
request_disk = 2GB

queue
