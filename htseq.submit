universe = vanilla
executable = htseq.sh
arguments = "$(RUN) $(ANNOT_GFF)"

output = htseq_$(RUN).out
error = htseq_$(RUN).err
log = htseq_$(RUN).log

should_transfer_files = YES
when_to_transfer_output = ON_EXIT
transfer_input_files = $(RUN).sort.bam

request_cpus = 4
request_memory = 2GB
request_disk = 2GB

queue
